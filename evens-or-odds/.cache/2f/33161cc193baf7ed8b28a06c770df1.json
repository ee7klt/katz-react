,{"generated":{"line":95,"column":22},"source":"components/App.js","original":{"line":67,"column":24}},{"generated":{"line":95,"column":25},"source":"components/App.js","original":{"line":67,"column":27}},{"name":"mapStateToProps","generated":{"line":95,"column":50},"source":"components/App.js","original":{"line":67,"column":35}},{"generated":{"line":95,"column":65},"source":"components/App.js","original":{"line":67,"column":27}},{"name":"mapDispatchToProps","generated":{"line":95,"column":67},"source":"components/App.js","original":{"line":67,"column":52}},{"generated":{"line":95,"column":85},"source":"components/App.js","original":{"line":67,"column":27}},{"generated":{"line":95,"column":86},"source":"components/App.js","original":{"line":67,"column":0}},{"name":"componentConnector","generated":{"line":97,"column":15},"source":"components/App.js","original":{"line":70,"column":15}},{"generated":{"line":97,"column":33},"source":"components/App.js","original":{"line":70,"column":33}},{"name":"App","generated":{"line":97,"column":34},"source":"components/App.js","original":{"line":70,"column":34}},{"generated":{"line":97,"column":37},"source":"components/App.js","original":{"line":70,"column":33}}],"sources":{"components/App.js":"import React, {Component} from 'react';\nimport {connect} from 'react-redux';\nimport {cancelGame,startGame,expandInstructions,collapseInstructions} from '../actions/settings';\nimport Instructions from './Instructions';\n\n\nclass App extends Component {\n\n\n  render() {\n    console.log('this',this);\n    return (\n      <div >\n      <h2>Evens or Odds</h2>\n      {\n        this.props.gameStarted ? (\n          <div>\n            <h3>The game is on!</h3>\n            <br />\n            <button onClick={this.props.cancelGame}>Cancel Game</button>\n          </div>\n        ) : (\n          <div>\n          <h3>A new game awaits</h3>\n          <br />\n          <button onClick={this.props.startGame}>Start Game</button>\n          </div>\n        )\n      }\n      {\n        <div>\n        {\n          this.props.instructionsExpanded ?\n          <div>\n            <Instructions />\n              <button onClick={this.props.collapseInstructions}>Collapse instructions</button>\n          </div> :\n          <button onClick={this.props.expandInstructions}>Expand instructions</button>\n\n        }\n        </div>\n      }\n      </div>\n\n    )\n  }\n}\n\n\nconst mapStateToProps = state => {\n  console.log('state',state);\n  return {\n    gameStarted: state.gameStarted,\n    instructionsExpanded: state.instructionsExpanded\n  }\n}\n\nconst mapDispatchToProps = dispatch => {\n  return {\n    startGame: () => dispatch(startGame()),\n    cancelGame: () => dispatch(cancelGame()),\n    expandInstructions: () => dispatch(expandInstructions()),\n    collapseInstructions: () => dispatch(collapseInstructions())\n  }\n}\n\nconst componentConnector = connect(mapStateToProps, mapDispatchToProps);\n\n\nexport default componentConnector(App);\n"},"lineCount":null}},"hash":"dff15bd5b0b24969425ed927e894d877","cacheData":{"env":{}}}