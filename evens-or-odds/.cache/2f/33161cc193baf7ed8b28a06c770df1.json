lumn":2}},{"generated":{"line":131,"column":6},"source":"components/App.js","original":{"line":102,"column":15}}],"sources":{"components/App.js":"import React, {Component} from 'react';\nimport {connect} from 'react-redux';\nimport {cancelGame} from '../actions/settings';\nimport {fetchNewDeck} from '../actions/deck';\nimport fetchStates from '../reducers/fetchStates';\nimport DrawCard from './DrawCard';\nimport ShowCard from './ShowCard';\nimport Instructions from './Instructions';\n\nclass App extends Component {\n\n\n\n  render() {\n\n\n  //   if (this.props.isFetching) {\n  //     return (\n  //       <div className='container-fluid'>\n  //       <h2>Evens or Odds</h2>\n  //\n  //       <div className=\"spinner-grow\" role=\"status\">\n  //\n  //       <span className=\"sr-only\">Loading...</span>\n  //       </div>\n  //       </div>\n  //     )\n  //   }\n  // else\n   if (this.props.fetchState === fetchStates.error) {\n      return (\n        <div className='container-fluid'>\n        <h2>Evens or Odds</h2>\n        <p>{this.props.message}</p>\n        </div>\n      )\n    }\n\n\n    else return (\n      <div className='container-fluid'>\n      <h2>Evens or Odds</h2>\n\n      {\n        this.props.gameStarted ? (\n          <div>\n            <h3>The game is on!</h3>\n            <br />\n            {\n              (this.props.isFetching) &&\n              <div className=\"spinner-grow\" role=\"status\">\n                <span className=\"sr-only\">Loading...</span>\n              </div>\n            }\n            {\n              <div className=\"card\">\n    <div className=\"card-body\">\n      <h5 className=\"card-title\">Card title</h5>\n\n  <ShowCard />\n          <DrawCard />\n    </div>\n  </div>\n            }\n\n\n          <hr />\n            <button type=\"button\" className=\"btn btn-primary\" onClick={this.props.cancelGame}>Cancel Game</button>\n          </div>\n        ) : (\n          <div>\n          <h3>A new game awaits</h3>\n          <br />\n          <button type=\"button\" className=\"btn btn-primary\" onClick={this.props.fetchNewDeck}>Start Game</button>\n          </div>\n        )\n      }\n      <hr />\n    <Instructions />\n      </div>\n\n    )\n  }\n}\n\n\nconst mapStateToProps = state => {\n  const {gameStarted} = state.settings;\n  const {fetchState, message, isFetching} = state.deck;\n  return  {gameStarted, fetchState, message, isFetching};\n}\n\n// const mapDispatchToProps = dispatch => {\n//   return {\n//     startGame: () => dispatch(startGame()),\n//     cancelGame: () => dispatch(cancelGame()),\n//     fetchNewDeck: () => fetchNewDeck(dispatch)\n//   }\n// }\n\n\nexport default connect(\n  mapStateToProps,\n  {cancelGame, fetchNewDeck}\n)(App);\n"},"lineCount":null}},"hash":"0aeb2cd38841858b167984afb907577c","cacheData":{"env":{}}}